 <filters>
  <format>
   <active>true</active>
   <policy>17</policy>
   <result>&lt;mypre>\0\&lt;/mypre></result>
  </format>
  <wall>
   <active>true</active>
   <policy>0</policy>
   <result>:browser:Wall: \1\&lt;font face=fixed size=3>\2\&lt;/font>\3\</result>
   <begin>^(&lt;.*>)?(&amp;lt Mtp&amp;gt  Wall :)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?&amp;lt Mtp&amp;gt  End of Wall(&lt;.*>)?</end>
   <depend>wall_emit</depend>
  </wall>
  <finger>
   <active>true</active>
   <policy>0</policy>
   <result>:id:Finger: \0\</result>
   <begin>^(&lt;.*>)?(Login *: \w+)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?&amp;lt Mtp&amp;gt  End of finger(&lt;.*>)?</end>
  </finger>
  <info>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font face=fixed size=2>\2\&lt;/font>\3\</result>
   <begin>^(&lt;.*>)?([^&amp;&lt;][^&lt;]*|&amp;lt Mtp&amp;gt  History[^&lt;]*|&amp;lt Mtp&amp;gt  System[^&lt;]*|&amp;lt Mtp&amp;gt  Help[^&lt;]*)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?&amp;lt [^&lt;]*(&lt;.*>)?</end>
  </info>
  <test_file>
   <active>true</active>
   <policy>0</policy>
   <result>\0\
:file:File:\3\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(\|\w+\| \[Rainbow\][^&lt;]*)(&lt;.*>)?</regexp>
  </test_file>
  <ignore_warning>
   <active>true</active>
   <policy>2147483647</policy>
   <result></result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  \w+ is away and may not be hearing you(&lt;.*>)?</regexp>
  </ignore_warning>
  <join_dessin>
   <active>true</active>
   <policy>2147483647</policy>
   <result>\0\
:drawing:Dessin:
:affect:Affectations:channel=Dessin</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You join channel Dessin(&lt;.*>)?</regexp>
  </join_dessin>
  <join_others>
   <active>true</active>
   <policy>2147483647</policy>
   <result>\0\
