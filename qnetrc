 <filters>
  <format>
   <active>true</active>
   <policy>0</policy>
   <result>&lt;mypre>\0\&lt;/mypre></result>
  </format>
  <wall>
   <active>true</active>
   <policy>0</policy>
   <result>:browser:Wall: \1\&lt;font face=fixed size=3>\2\&lt;/font>\3\</result>
   <beginresult>:browser:Wall:</beginresult>
   <endresult>:browser:Wall:</endresult>
   <begin>^(&lt;.*>)?(&amp;lt Mtp&amp;gt  Wall :)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?(&amp;lt Mtp&amp;gt  End of Wall)(&lt;.*>)?</end>
   <depend>wall_emit</depend>
  </wall>
  <finger>
   <active>true</active>
   <policy>0</policy>
   <result>:browser:Finger: \0\</result>
   <beginresult>:browser:Finger: \0\</beginresult>
   <endresult>:browser:Finger: \0\</endresult>
   <begin>^(&lt;.*>)?(Login *: \w+)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?&amp;lt Mtp&amp;gt  End of finger(&lt;.*>)?</end>
  </finger>
  <info>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font face=fixed size=2>\2\&lt;/font>\3\</result>
   <beginresult>\1\&lt;font face=fixed size=2>\2\&lt;/font>\3\</beginresult>
   <endresult>\1\&lt;font face=fixed size=2>\2\&lt;/font>\3\</endresult>
   <begin>^(&lt;.*>)?([^&amp;&lt;][^&lt;]*|&amp;lt Mtp&amp;gt  History[^&lt;]*|&amp;lt Mtp&amp;gt  System[^&lt;]*|&amp;lt Mtp&amp;gt  Help[^&lt;]*)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?(&amp;lt [^&lt;]*)(&lt;.*>)?</end>
  </info>
  <ignore_warning>
   <active>true</active>
   <policy>0</policy>
   <result/>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  \w+ is away and may not be hearing you(&lt;.*>)?</regexp>
  </ignore_warning>
  <join_dessin>
   <active>true</active>
   <policy>0</policy>
   <result>\0\
:drawing:Dessin:
:affect:Affectations:channel=Dessin</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You join channel Dessin(&lt;.*>)?</regexp>
  </join_dessin>
  <join_others>
   <active>true</active>
   <policy>0</policy>
   <result>\0\
:affect:Affectations:channel=\3\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You join channel (\w+)(&lt;.*>)?</regexp>
  </join_others>
  <leave_channel>
   <active>true</active>
   <policy>0</policy>
   <result>\0\
:affect:Affectations:channel=Hall</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You leave channel (\w+)(&lt;.*>)?</regexp>
  </leave_channel>
  <draw_other>
   <active>true</active>
   <policy>0</policy>
   <result>:drawing:Dessin:\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt \w+&amp;gt  )(\}[LCT][^&lt;]*)(&lt;.*>)?</regexp>
  </draw_other>
  <tell_receive>
   <active>true</active>
   <policy>0</policy>
   <result>:tell:\4\: \1\&amp;lt \4\&amp;gt \6\\7\
:splash:spl:[SE]&lt;\4\> \6\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  )(\w+) (tells you:|asks you:|replies:)([^&lt;]*)(&lt;.*>)?</regexp>
  </tell_receive>
  <tell_emit>
   <active>true</active>
   <policy>0</policy>
   <result>:tell:\4\: \1\&lt;font color=darkblue>&amp;lt \Login\&amp;gt \6\&lt;/font>\7\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You (tell|ask|reply to) (\w+)(:)(.*)(&lt;.*>)?</regexp>
  </tell_emit>
  <notify_moves>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=blue>\2\\3\\4\ \5\\6\\7\&lt;/font>
:tell?:\4\: \1\&lt;font color=blue>\3\\4\ \5\\6\&lt;/font>\7\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  )(\w+) (is away|is back|leaves|disconnects|is kicked out|comes in)(.*)(&lt;.*>)?</regexp>
  </notify_moves>
  <mytalk>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=darkblue>\2\\3\&lt;/font>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt \login\&amp;gt .*)(&lt;.*>)?</regexp>
  </mytalk>
  <emote>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=darkgreen>\2\\3\&lt;/font>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  \*.*)(&lt;.*>)?</regexp>
  </emote>
  <me>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=red>\2\\3\&lt;/font>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt .*you:.*)(&lt;.*>)?</regexp>
  </me>
  <other>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=magenta>\2\\3\&lt;/font>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  You.*)(&lt;.*>)?</regexp>
  </other>
  <server>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=blue>\2\\3\&lt;/font>\4\</result>
