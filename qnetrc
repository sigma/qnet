  </leave_channel>
  <draw_other>
   <active>true</active>
   <policy>16</policy>
   <result>:drawing:Dessin:\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt \w+&amp;gt  )(\}[LCT][^&lt;]*)(&lt;.*>)?</regexp>
  </draw_other>
  <tell_receive>
   <active>true</active>
   <policy>2147483647</policy>
   <result>:tell:\4\: \1\&amp;lt \4\&amp;gt \6\\7\
:splash:spl:&lt;\4\> \6\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  )(\w+) (tells you:|asks you:|replies:)([^&lt;]*)(&lt;.*>)?</regexp>
  </tell_receive>
  <tell_emit>
   <active>true</active>
   <policy>3407964</policy>
   <result>:tell:\4\: \1\&lt;font color=darkblue>&amp;lt \Login\&amp;gt \6\&lt;/font>\7\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You (tell|ask|reply to) (\w+)(:)(.*)(&lt;.*>)?</regexp>
  </tell_emit>
  <notify_moves>
   <active>true</active>
   <policy>2097244</policy>
   <result>\1\&lt;font color=blue>\2\\3\\4\ \5\\6\\7\&lt;/font>
:tell?:\4\: \1\&lt;font color=blue>\3\\4\ \5\\6\&lt;/font>\7\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  )(\w+) (is away|is back|leaves|disconnects|is kicked out|comes in)(.*)(&lt;.*>)?</regexp>
  </notify_moves>
  <mytalk>
