 <filters>
  <format>
   <active>true</active>
   <policy>0</policy>
   <result>&lt;mypre>\0\&lt;/mypre></result>
  </format>
  <wall>
   <active>true</active>
   <policy>0</policy>
   <result>:browser:Wall: \1\&lt;block>\2\&lt;/block>\3\</result>
   <beginresult>:browser:Wall: &lt;hr></beginresult>
   <endresult/>
   <begin>^(&lt;.*>)?(&amp;lt Mtp&amp;gt  Wall :)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?(&amp;lt Mtp&amp;gt  End of Wall)(&lt;.*>)?</end>
  </wall>
  <finger>
   <active>true</active>
   <policy>0</policy>
   <result>:browser:Finger: \0\</result>
   <beginresult>:browser:Finger: \0\</beginresult>
   <endresult>:browser:Finger: \0\</endresult>
   <begin>^(&lt;.*>)?(Login *: \w+)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?(&amp;lt Mtp&amp;gt  End of finger)(&lt;.*>)?</end>
  </finger>
  <info>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;block>\2\&lt;/block>\3\</result>
   <beginresult>\1\&lt;block>\2\&lt;/block>\3\</beginresult>
   <endresult>\1\\2\\3\</endresult>
   <begin>^(&lt;.*>)?([^|&amp;&lt;][^&lt;]*|&amp;lt Mtp&amp;gt  History[^&lt;]*|&amp;lt Mtp&amp;gt  System[^&lt;]*|&amp;lt Mtp&amp;gt  Help[^&lt;]*)(&lt;.*>)?</begin>
   <end>^(&lt;.*>)?(&amp;lt [^&lt;]*)(&lt;.*>)?</end>
  </info>
  <incoming>
   <active>true</active>
   <policy>0</policy>
   <result>&lt;other>\0\&lt;/other>
:emit:Emit:wall
</result>
   <regexp>(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  (Your client is now |You set client to ).*(&lt;.*>)?</regexp>
  </incoming>
  <pong>
   <active>false</active>
   <policy>1</policy>
   <result>:cron:Cron:restart</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?\|\w+\| ping(&lt;.*>)?</regexp>
  </pong>
  <listen>
   <active>true</active>
   <policy>0</policy>
   <result>:emit:Emit:senddata \3\ [Rainbow][Song][\3\ en slip.ogg]</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?\|(\w+)\| \[Rainbow\]\[Listen\](&lt;.*>)?</regexp>
  </listen>
  <version>
   <active>true</active>
   <policy>0</policy>
   <result>:emit:Emit:senddata \3\ KACK VERSION Win95 Telnet (Ultimate Edition)</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?\|(\w+)\| KREQ VERSION(&lt;.*>)?</regexp>
  </version>
  <test_file>
   <active>false</active>
   <policy>0</policy>
   <result>\0\
:file:File:\3\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(\|\w+\| \[Rainbow\][^&lt;]*)(&lt;.*>)?</regexp>
  </test_file>
  <ignore_warning>
   <active>true</active>
   <policy>0</policy>
   <result/>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  \w+ is away and may not be hearing you(&lt;.*>)?</regexp>
  </ignore_warning>
  <join_dessin>
   <active>true</active>
   <policy>0</policy>
   <result>\0\
:drawing:Dessin:
:affect:Affectations:channel=Dessin</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You join channel Dessin(&lt;.*>)?</regexp>
  </join_dessin>
  <join_others>
   <active>true</active>
   <policy>0</policy>
   <result>\0\
:affect:Affectations:channel=\3\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You join channel (\w+)(&lt;.*>)?</regexp>
  </join_others>
  <leave_channel>
   <active>true</active>
   <policy>0</policy>
   <result>\0\
:affect:Affectations:channel=Hall</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You leave channel (\w+)(&lt;.*>)?</regexp>
  </leave_channel>
  <draw_other>
   <active>true</active>
   <policy>0</policy>
   <result>:drawing:Dessin:\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt \w+&amp;gt  )(\}[LCT][^&lt;]*)(&lt;.*>)?</regexp>
  </draw_other>
  <tell_receive>
   <active>true</active>
   <policy>0</policy>
   <result>-:tell:\4\: \1\&amp;lt \4\&amp;gt \6\\7\
-:splash:spl:[SE]&lt;\4\> \6\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  )(\w+) (tells you:|asks you:|replies:)([^&lt;]*)(&lt;.*>)?</regexp>
  </tell_receive>
  <tell_emit>
   <active>true</active>
   <policy>0</policy>
   <result>-:tell:\4\: \1\&lt;me>&amp;lt \Login\&amp;gt \6\&lt;/me>\7\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You (tell|ask|reply to) (\w+)(:)(.*)(&lt;.*>)?</regexp>
  </tell_emit>
  <notify_moves>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;server>\2\\3\\4\ \5\\6\\7\&lt;/server>
:tell?:\4\: \1\&lt;server>\3\\4\ \5\\6\&lt;/server>\7\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  )(\w+) (is away|is back|leaves|disconnects|is kicked out|comes in)(.*)(&lt;.*>)?</regexp>
  </notify_moves>
  <mytalk>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;me>\2\\3\&lt;/me>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt \login\&amp;gt .*)(&lt;.*>)?</regexp>
  </mytalk>
  <emote>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;emote>\2\\3\&lt;/emote>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  \*.*)(&lt;.*>)?</regexp>
  </emote>
  <me>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;aboutme>\2\\3\&lt;/aboutme>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt .*you:.*)(&lt;.*>)?</regexp>
  </me>
  <mywall>
   <active>true</active>
   <policy>0</policy>
   <result>:ptime:ok:DATE&lt;-[date]dd/MM/yy
:ptime:ok:TIME&lt;-[time]hh:mm:ss
:pformat:ko:LOGIN&lt;-&quot;%1&quot;,(&quot;\Login\&quot;:-8)
:browser:Wall:\1\ &lt;block>\DATE\ \TIME\ \LOGIN\ \3\&lt;/block>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  You write to the wall: (.*)(&lt;.*>)?</regexp>
  </mywall>
  <otherwall>
   <active>true</active>
   <policy>0</policy>
   <result>:ptime:ok:DATE&lt;-[date]dd/MM/yy
:ptime:ok:TIME&lt;-[time]hh:mm:ss
:pformat:ko:LOGIN&lt;-&quot;%1&quot;,(&quot;\3\&quot;:-8)
:browser:Wall:\1\ &lt;block>\DATE\ \TIME\ \LOGIN\ \4\&lt;/block>\5\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt  (\w+) writes to the wall: (.*)(&lt;.*>)?</regexp>
  </otherwall>
  <other>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;other>\2\\3\&lt;/other>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt  You.*)(&lt;.*>)?</regexp>
  </other>
  <server>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;server>\2\\3\&lt;/server>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt Mtp&amp;gt .*)(&lt;.*>)?</regexp>
  </server>
  <aboutme>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;aboutme>\2\\3\\4\&lt;/aboutme>\6\
-:splash:spl:[SE]\2\\3\\4\</result>
   <regexp>(:\w+:\w+:)?(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt .*(\login\)[^&lt;]*)(&lt;.*>)?</regexp>
  </aboutme>
  <default>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;default>\2\&lt;/default></result>
   <regexp>(:\w+:\w+:)?(.*)</regexp>
  </default>
  <urls>
   <active>true</active>
   <policy>1</policy>
   <result>&lt;url>&lt;a href=&quot;\1\\2\&quot;>\1\\2\&lt;/a>&lt;/url>\3\</result>
   <regexp>(mailto:|ftp://|http://)([^&quot; &lt;]*)([&quot;
&lt;])</regexp>
  </urls>
  <wall_emit>
   <active>true</active>
   <policy>1</policy>
   <result>\0\</result>
   <regexp>wall</regexp>
   <memorize>true</memorize>
  </wall_emit>
