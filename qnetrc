  </server>
  <aboutme>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=red>\2\\3\\4\\5\&lt;/font>
:splash:spl:[SE]\2\\3\\4\</result>
   <regexp>(:\w+:\w+:)?(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?(&amp;lt .*\login\[^&lt;]*)(&lt;.*>)?</regexp>
  </aboutme>
  <default>
   <active>true</active>
   <policy>0</policy>
   <result>\1\&lt;font color=black>\2\&lt;/font></result>
   <regexp>(:\w+:\w+:)?(.*)</regexp>
  </default>
  <urls>
   <active>true</active>
   <policy>1</policy>
   <result>&lt;font color=&quot;blue&quot;>&lt;a href=&quot;\1\\2\&quot;>\1\\2\&lt;/a>&lt;/font>\3\</result>
   <regexp>(mailto:|ftp://|http://)([^&quot; &lt;]*)([&quot; 
&lt;])</regexp>
  </urls>
  <wall_emit>
   <active>true</active>
   <policy>1</policy>
   <result>\0\</result>
   <regexp>wall</regexp>
   <memorize>true</memorize>
  </wall_emit>
  <mywall>
   <active>true</active>
   <policy>0</policy>
   <result>:ptime:ok:DATE&lt;-[date]dd/MM/yyyy
:ptime:ok:TIME&lt;-[time]hh:mm:ss
:pformat:ko:LOGIN&lt;-&quot;%1&quot;,(&quot;\Login\&quot;:-8)
:browser:Wall:\1\&lt;font face=fixed size=3>\DATE\ \TIME\ \LOGIN\ \3\&lt;/font>\4\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt You write to the wall: (.*)(&lt;.*>)?</regexp>
  </mywall>
  <otherwall>
   <active>true</active>
   <policy>0</policy>
   <result>:ptime:ok:DATE&lt;-[date]dd/MM/yyyy
:ptime:ok:TIME&lt;-[time]hh:mm:ss
:pformat:ko:LOGIN&lt;-&quot;%1&quot;,(&quot;\3\&quot;:-8)
:browser:Wall:\1\&lt;font face=fixed size=3>\DATE\ \TIME\ \LOGIN\ \4\&lt;/font>\5\</result>
   <regexp>^(&lt;.*>)?(\d{2}:\d{2}:\d{2} )?&amp;lt Mtp&amp;gt (\w+) writes to the wall: (.*)(&lt;.*>)?</regexp>
